<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>planners</name>
    <version>0.0.0</version>
    <description>Path Planner for mapped tracks</description>
    <maintainer email="team@qutmotorsport.com">Alastair Bradford</maintainer>
    <license>MIT</license>

    <depend>rclpy</depend>

    <!-- Messages -->
    <depend>driverless_msgs</depend>
    <depend>geometry_msgs</depend>
    <depend>nav_msgs</depend>
    <depend>diagnostic_msgs</depend>

    <!-- Custom packages -->
    <depend>driverless_common</depend>

    <!-- Python -->
    <depend>python3-numpy</depend>
    <depend>python3-scipy</depend>
    <depend>python3-transforms3d</depend>

    <depend>tf2</depend>
    <depend>tf2_ros</depend>

    <export>
        <build_type>ament_python</build_type>
    </export>
</package>
