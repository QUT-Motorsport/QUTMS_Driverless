<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>slam_gridmap</name>
  <version>1.0.0</version>
  <description>real-time SLAM cone detection operating on gridmaps</description>
  <maintainer email="team@qutmotorsport.com">Claudia Kiss</maintainer>
  <license>MIT</license>

  <depend>rclpy</depend>

    <!-- Messages -->
    <depend>nav_msgs</depend>
    <depend>geometry_msgs</depend>
    <depend>driverless_msgs</depend>

    <!-- Custom packages -->
    <depend>driverless_common</depend>

    <!-- Python -->
    <depend>python3-numpy</depend>
    <depend>python3-sklearn</depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
