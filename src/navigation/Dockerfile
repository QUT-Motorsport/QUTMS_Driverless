FROM qutms_driverless_base
# qutms_driverless_base automatically generated by makefile

# copy in navigation files and install Python Dependencies
COPY --chown=${HOST_UID}:${HOST_GROUP} ./src/navigation /home/${USERNAME}/driverless_ws/src/navigation
RUN pip install -r /home/${USERNAME}/driverless_ws/src/navigation/requirements.txt
# remove files
RUN rm -r /home/${USERNAME}/driverless_ws/src/*
