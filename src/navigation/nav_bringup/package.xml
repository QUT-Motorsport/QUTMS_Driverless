<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>nav_bringup</name>
    <version>0.0.0</version>
    <description>Nav2 for Formula Student</description>
    <maintainer email="team@qutmotorsport.com">Alastair Bradford, Grant van Breda</maintainer>
    <license>MIT</license>

    <buildtool_depend>ament_cmake</buildtool_depend>

    <exec_depend>robot_localization</exec_depend>
    <exec_depend>slam_toolbox</exec_depend>
    <exec_depend>navigation2</exec_depend>
    <exec_depend>sbg_driver</exec_depend>
    <!-- Custom packages -->
    <exec_depend>odom_transformer</exec_depend>

    <export>
        <build_type>ament_cmake</build_type>
    </export>
</package>
