<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>slam</name>
    <version>0.0.0</version>
    <description>Contains Implementations of SLAM for Track Mapping and Navigation</description>
    <maintainer email="team@qutmotorsport.com">Alistair English</maintainer>
    <license>MIT</license>

    <buildtool_depend>ament_cmake</buildtool_depend>

    <depend>rclcpp</depend>
    <depend>std_msgs</depend>
    <depend>sensor_msgs</depend>
    <depend>geometry_msgs</depend>
    <depend>ackermann_msgs</depend>
    <depend>driverless_msgs</depend>
    <depend>message_filters</depend>
    <depend>sim_translators</depend>
    <depend>visualization_msgs</depend>

    <export>
        <build_type>ament_cmake</build_type>
    </export>
</package>
