<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>vehicle_bringup</name>
    <version>0.0.0</version>
    <description>Vehicle operations and supervisor</description>
    <maintainer email="team@qutmotorsport.com">Alastair Bradford, Walden Leung, Alex Bradbury</maintainer>
    <license>MIT</license>

    <depend>rosbag2_storage_mcap</depend>
    <depend>foxglove_bridge</depend>
    <depend>rmw_cyclonedds_cpp</depend>
    <depend>velodyne</depend>
    <depend>pointcloud_to_laserscan</depend>


    <!-- Custom packages -->
    <depend>driverless_common</depend>
    <depend>QUTMS_Embedded_Common</depend>
    <depend>rosboard</depend>
    <depend>controllers</depend>
    <depend>steering_actuator</depend>
    <depend>canbus</depend>
    <depend>vehicle_urdf</depend>
    <depend>terminal_control</depend>
    <depend>planners</depend>
    <depend>map_creation</depend>
    <depend>nav_bringup</depend>
    <depend>lidar_pipeline</depend>
    <depend>velocity_controller</depend>
    <depend>odom_transformer</depend>

    <!-- Messages -->
    <depend>driverless_msgs</depend>
    <depend>lifecycle_msgs</depend>
    <depend>std_msgs</depend>
    <depend>nav_msgs</depend>
    <depend>geometry_msgs</depend>
    <depend>diagnostic_msgs</depend>
    <depend>sensor_msgs</depend>

    <exec_depend>ros2launch</exec_depend>

    <export>
        <build_type>ament_python</build_type>
    </export>
</package>
