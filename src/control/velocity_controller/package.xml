<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>velocity_controller</name>
    <version>0.0.1</version>
    <description>Autonomous System Velocity Controller</description>
    <maintainer email="team@qutmotorsport.com">Calvin Johnson</maintainer>
    <license>MIT</license>

    <depend>rclcpp</depend>

    <!-- Messages -->
    <depend>ackermann_msgs</depend>
    <depend>nav_msgs</depend>
    <depend>rcl_interfaces</depend>
    <depend>driverless_msgs</depend>
    <!-- Packages -->
    <depend>QUTMS_Embedded_Common</depend>

    <buildtool_depend>ament_cmake</buildtool_depend>

    <export>
        <build_type>ament_cmake</build_type>
    </export>
</package>
