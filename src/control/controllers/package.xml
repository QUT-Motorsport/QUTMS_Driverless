<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>controllers</name>
    <version>0.0.0</version>
    <description>Driverless Controllers</description>
    <maintainer email="team@qutmotorsport.com">Alistair English, Alastair Bradford, Grant van Breda</maintainer>
    <license>MIT</license>

    <!-- Messages -->
    <depend>driverless_msgs</depend>
    <depend>ackermann_msgs</depend>
    <depend>builtin_interfaces</depend>
    <depend>geometry_msgs</depend>
    <depend>sensor_msgs</depend>
    <depend>visualization_msgs</depend>
    <!-- Custom packages -->
    <depend>driverless_common</depend>

    <export>
        <build_type>ament_python</build_type>
    </export>
</package>
