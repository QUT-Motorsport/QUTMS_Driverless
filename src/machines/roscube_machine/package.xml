<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>roscube_machine</name>
    <version>0.0.0</version>
    <description>Machine Packge for ROSCube</description>
    <maintainer email="team@qutmotorsport.com">Alistair English</maintainer>
    <license>MIT</license>

    <buildtool_depend>ament_cmake</buildtool_depend>

    <depend>rosbag2_storage_mcap</depend>
    <depend>foxglove_bridge</depend>
    <depend>rmw_cyclonedds_cpp</depend>
    <depend>velodyne</depend>
    <depend>pointcloud_to_laserscan</depend>

    <!-- Packages -->
    <depend>rosboard</depend>
    <depend>controllers</depend>
    <depend>steering_actuator</depend>
    <depend>canbus</depend>
    <depend>vehicle_urdf</depend>
    <depend>vehicle_supervisor</depend>
    <depend>terminal_control</depend>
    <depend>planners</depend>
    <depend>slam_gridmap</depend>
    <depend>nav_bringup</depend>
    <depend>sensors</depend>
    <depend>driverless_common</depend>
    <depend>lidar_pipeline</depend>
    <depend>velocity_controller</depend>
    <depend>mission_controller</depend>
    <depend>odom_transformer</depend>
    <depend>sbg_driver</depend>
    <depend>map_creation</depend>

    <exec_depend>ros2launch</exec_depend>

    <export>
        <build_type>ament_cmake</build_type>
    </export>
</package>
