<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>jetson_machine</name>
    <version>0.0.0</version>
    <description>Machine Package for Nvidia Jetson</description>
    <maintainer email="team@qutmotorsport.com">Alistair English</maintainer>
    <license>MIT</license>

    <buildtool_depend>ament_cmake</buildtool_depend>

    <!-- Packages -->
    <depend>vision_pipeline</depend>
    <!-- This is cloned in the dockerfile -->
    <depend>zed_ros2</depend>

    <exec_depend>ros2launch</exec_depend>

    <export>
        <build_type>ament_cmake</build_type>
    </export>
</package>
