<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>points_maker</name>
  <version>0.0.0</version>
  <description>Segments ground planes and separates objects</description>
  <author email="alistair.e@cmpeng.com.au">Alistair English</author>
  <maintainer email="team@qutmotorsport.com">Alastair Bradford</maintainer>
  <license>TODO: License declaration</license>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>sensor_msgs</depend>
  <depend>pcl_ros</depend>
  <depend>pcl_conversions</depend>

  <exec_depend>libpcl-common</exec_depend>
  <exec_depend>libpcl-io</exec_depend>
  <exec_depend>libpcl-filters</exec_depend>
  <exec_depend>libpcl-segmentation</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
